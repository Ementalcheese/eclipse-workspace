package override;

 class Shape{
	public Shape next;
	public Shape() { next = null;}
	
	public void draw()
	{
		System.out.println("Shape");
		
	}
	
 class Line extends Shape{
	 public void draw()
	 {
		 System.out.println("Line");
	 }
	 
 }
	
	
	
	
	
	
	
}





public class OverridingEX {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
