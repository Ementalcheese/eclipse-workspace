package exam;

import java.util.Scanner;

public class Q14 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int arr[];   //배열 선언
		int num;
		
		System.out.println("정수");
		
		num = sc.nextInt();
		
		arr = new int [num];   //배열 생성 (배열의 index에 방을 생성)
		
		
		
		
		
		
		
		sc.close();
	}

}
