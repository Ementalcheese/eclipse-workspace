package upcasting;

public class UpcastingEx {

	public static void main(String[] args) {
		
		Person p;
		Student s = new Student("홍길동");
		
		p = s;      //업캐스팅
		
		System.out.println(p.name);
		
		//p.grade = "A";         //컴파일 오류
		//p.department = "HR";   //컴파일 오류
		p.name = "이순신";
		p.id = "ID";
		
		s.grade = "A";
		s.department = "HR";
		
		
		
		
	}

}
