package array;

public class ArrayTest3 {

	public static void main(String[] args) {
		
		
		
		
		double[] data = new double[5]; // 배열 선언

		data[0] = 10.0;
		data[1] = 20.0;
		data[2] = 30.0;

		
			
		
		for (int i = 0; i < data.length; i++) {
			System.out.println(data[i]);
			total *= data[i];
		
		}
		
		double total = 1;
		System.out.println(total); //곱하는 경우 문제 발생.

	}

}
